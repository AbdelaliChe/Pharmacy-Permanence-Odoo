<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="report_my_pharmacy_template">
        <t t-call="web.html_container">
            <t t-call="web.external_layout">
                <div class="page">
                    <h2>Permanence Pharmacies in 
                    </h2>
                    <table class="table table-condensed">
                        <thead>
                            <tr>
                                <th>Name</th>
                                <th>Address</th>
                                <th>city</th>
                                <th>country</th>
                                <th>Date Start Permanence</th>
                                <th>Date End Permanence</th>
                            </tr>
                        </thead>
                        <tbody>
                            <t t-foreach="docs" t-as="o">
                                <tr>
                                    <td>
                                        <t t-esc="o.name"/>
                                    </td>
                                    <td>
                                        <t t-esc="o.address"/>
                                    </td>
                                    <td>
                                        <t t-esc="o.city"/>
                                    </td>
                                    <td>
                                        <t t-esc="o.country"/>
                                    </td>
                                    <td>
                                        <t t-esc="o.dateStartPermanence"/>
                                    </td>
                                    <td>
                                        <t t-esc="o.dateEndPermanence"/>
                                    </td>
                                </tr>
                            </t>
                        </tbody>
                    </table>
                </div>
            </t>
        </t>
    </template>
</odoo>
